BINARIES := cjs-ledger cjs-manifest

build:

Makefile: configure Makefile.in
	./config.status

install:
	install --target-directory=$(DESTDIR)@bindir@ $(BINARIES)

clean:


distclean:
	@rm -f Makefile config.status

release:
	tools/release.sh


TEST_MYPY = $(addprefix test-mypy-,$(BINARIES))
test: $(TEST_MYPY)

$(TEST_MYPY): test-mypy-%: %
	mypy --strict $<

.PHONY: test $(TEST_MYPY)
